PROJECTS="edge|react-rails-tutorial|setler|coffee-rails|password_strength|rails-uptodate|rails-uptodate5edge|testgithub|testing-app|testing-app5edge"
if [ "X$1X" == "XX" ] ; then
    ls -lt ${HOME}/Projects |egrep ${PROJECTS} \
    | egrep -v "`date '+%b %d'`|`date '+%b  %-d'`"
else
    ls -lt ${HOME}/Projects |egrep ${PROJECTS} \
    | egrep -v "DEFAULT|$1"
fi
